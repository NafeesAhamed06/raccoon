        .blogss {
          font-size: 2rem;
          font-weight: 700;
          letter-spacing: 2px;
          color: #ff00ff;
          text-shadow: 0 0 10px #ff00ff, 0 0 20px #9900ff;
          text-align: center;
          margin-bottom: 30px;
          margin-top: 30px;
        }

        /* Headings with neon glow */
        h2 {
          font-size: 1.4rem;
          margin-top: 40px;
          margin-bottom: 12px;
          color: #00e5ff;
          text-shadow: 0 0 8px #00e5ff, 0 0 16px #00e5ff;
          border-left: 4px solid #ff00ff;
          padding-left: 10px;
        }

        p {
          font-size: 1rem;
          margin-bottom: 18px;
          color: #ddd;
        }

