        a {
          color: #ff00ff;
          text-shadow: 0 0 5px #ff00ff;
          text-decoration: none;
        }

        a:hover {
          text-shadow: 0 0 10px #ff00ff, 0 0 20px #ff00ff;
        }
        
        .ext {
          position: fixed;
          top: 1%;
          left: 3%;
          cursor: pointer;
          background: linear-gradient(135deg, #d27bff, #b84bff);
          height: 50px;
          width: 50px;
          display: flex;
          align-items: center;
          justify-content: center;
          border-radius: 50%;
          transition: all 0.25s ease-in-out;
          box-shadow: 0 4px 8px rgba(0, 0, 0, 0.15);
        }

        .ext img {
          width: 60%;
          height: auto;
          transition: transform 0.25s ease-in-out;
        }

        /* Hover effects */
        .ext:hover {
          transform: scale(1.1);
          background: linear-gradient(135deg, #b84bff, #8f3aff);
          box-shadow: 0 6px 12px rgba(0, 0, 0, 0.25);
        }

        .ext:hover img {
          transform: rotate(10deg);
        }

        /* Mobile Responsive */
        @media (max-width: 768px) {
          .main-container {
            padding: 20px;
          }
          h2 {
            font-size: 1.2rem;
          }
          .blogss {
            font-size: 1.6rem;
          }
        }
        @media (max-width: 420px) {
          .ext{
            margin: 0;
